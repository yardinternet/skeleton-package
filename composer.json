{
	"name": "yard/skeleton-package",
	"type": "package",
	"description": "A skeleton example package for Roots Acorn.",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Yard\\SkeletonPackage\\": "src/"
		}
	},
	"require": {
		"php": "^8.0",
		"spatie/laravel-package-tools": "^1.16"
	},
	"extra": {
		"acorn": {
			"providers": [
				"Yard\\SkeletonPackage\\ExampleServiceProvider"
			],
			"aliases": {
				"Example": "Yard\\SkeletonPackage\\Facades\\Example"
			}
		}
	}
}
